<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="2columns-left" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd" label="Customer My Account (All Pages)" design_abstraction="custom">
    <head>
        <title>MY PROFILE</title>
    </head>
    <body>
        <referenceBlock name="customer-account-navigation-account-edit-link">
            <arguments>
                <argument name="label" xsi:type="string" translate="true">Account Details</argument>
                <argument name="path" xsi:type="string">customer/account/edit</argument>
                <argument name="sortOrder" xsi:type="number">130</argument>
            </arguments>
        </referenceBlock>
        <referenceBlock name="customer-account-navigation-address-link">
            <arguments>
                <argument name="label" xsi:type="string" translate="true">Address Book</argument>
                <argument name="path" xsi:type="string">customer/address</argument>
                <argument name="sortOrder" xsi:type="number">120</argument>
            </arguments>
        </referenceBlock>
        <referenceBlock name="customer-account-navigation-wish-list-link">
            <arguments>
                <argument name="path" xsi:type="string">wishlist</argument>
                <argument name="label" xsi:type="string" translate="true">My Wish List</argument>
                <argument name="sortOrder" xsi:type="number">110</argument>
            </arguments>
        </referenceBlock>
       
        <!-- Downloadable product link -->
        <referenceBlock name="customer-account-navigation-downloadable-products-link" remove="true"/>
        <!-- Subscription link -->
        <referenceBlock name="customer-account-navigation-newsletter-subscriptions-link" remove="true"/>
        <!-- My credit card link -->
        <referenceBlock name="customer-account-navigation-my-credit-cards-link" remove="true"/>
        <referenceBlock name="catalog.compare.sidebar" remove="true"/>
        
    </body>
</page>
